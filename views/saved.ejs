<div class="row">
    <div class="col-3"></div>
<div class="col-5">
<table>
    <tr>
        <th class="col-4">
            التاريخ
        </th>
        <th class="col-3">
            المبلغ
        </th>
        <th class="col-3">
            عدد المستفيدين
        </th>
    </tr>
    <% order.forEach(element=>{%>
    <tr>
        <td>
            <%= element.header.date  %>
        </td>
        <td>
            <%= element.header.totalAmount  %>
        </td>
        <td>
            <%= element.header.personCount  %>
        </td>
        <th class="col-3">
            <a href="/generate/?id=<%= element._id%>" onclick="downloadClick"
                class="btn btn-outline-success">توليد</a>
        </th>
    <%})%>
    </tr>
</table>
</div>
<div class="col-2"></div>
</div>
<div class="col-2 btn btn-ourline-rounded"><a href="/">إنشاء عملية جديدة</a></div>